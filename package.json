{
  "name": "redux-create",
  "version": "1.0.8",
  "description": "redux create reducer, action creator, async action creator, action type",
  "main": "dist/index.js",
  "typings": "./index.d.ts",
  "repository": {
    "url": "https://github.com/marcelmokos/redux-create.git",
    "type": "git"
  },
  "authors": [
    "Marcel MokoÅ¡ (https://github.com/marcelmokos)",
    "Martin Litvaj (https://github.com/Kamahl19)"
  ],
  "keywords": [
    "react",
    "redux",
    "reducer",
    "create reducer",
    "action",
    "action type",
    "action creator",
    "async action creator",
    "typescript",
    "flow"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "clean": "rimraf dist coverage",
    "test": "yarn run lint:fix && cross-env BABEL_ENV=commonjs jest --testPathPattern /src/",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "yarn test -- --coverage",
    "test:travis:coverage": "cross-env BABEL_ENV=commonjs jest --testPathPattern /dist/ --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "prettier": "prettier --trailing-comma all --bracket-spacing=false --write 'flow-typed/index.js' '{src,spec,__{tests,mocks}__}/**/*.js'",
    "flow": "flow",
    "flow:coverage": "flow coverage ./src/index.js --color",
    "lint": "eslint \"src/**\" --cache",
    "lint:fix": "yarn run prettier && yarn run lint -- --fix",
    "lint:staged": "yarn lint:fix -- --rule 'no-console:2' --rule 'no-debugger:2'",
    "precommit": "lint-staged",
    "build": "babel src --out-dir dist",
    "prepublish": "npm run clean && npm run lint && npm run build && npm run test:travis:coverage",
    "release-patch": "npm version patch -m \"Upgrade to %s\" && npm publish",
    "release-minor": "npm version minor -m \"Upgrade to %s\" && npm publish",
    "release-major": "npm version major -m \"Upgrade to %s\" && npm publish"
  },
  "lint-staged": {
    "*.js": [
      "lint:staged",
      "git add"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-jest": "^20.0.3",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^2.11.15",
    "cross-env": "^5.0.0",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jest": "^20.0.3",
    "flow-bin": "^0.50.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "lint-staged": "^4.0.2",
    "prettier": "^1.3.1",
    "rimraf": "^2.6.1"
  }
}
